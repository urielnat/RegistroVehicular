<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Registro vehicular</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Registro vehicular", "Index", "Vehiculo", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>
                        <a href="@Url.Action("Create", "Vehiculo")">
                            <span class="glyphicon glyphicon-road"></span>
                            Dar de alta vehículo
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Create", "Placas")">
                            <span class="glyphicon glyphicon-list-alt"></span>
                            Definir patron
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Create", "Reserva")">
                            <span class="glyphicon glyphicon-ok"></span>
                            Reserva de placas por entidad
                        </a>
                    </li>
                
                </ul>
              
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Realizado por: Yonatan Uriel Hernández Montelongo</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
